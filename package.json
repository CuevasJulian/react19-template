{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "cra-template-typescript": "1.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.0.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "webpack --mode=production --node-env=production",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:css": "npx tailwindcss -i ./src/styles/index.css -o ./src/styles/output.css --watch",
    "build:css": "npx tailwindcss -i ./src/styles/index.css -o ./src/styles/output.css",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve --mode=development",
    "deploy:local": "npm run watch:css && npm run serve",
    "deploy:prod": "npm run build:css && npm run build:prod"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-private-property-in-object": "^7.25.9",
    "@eslint/js": "^9.16.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@webpack-cli/generators": "^3.0.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "file-loader": "^6.2.0",
    "prettier": "3.4.2",
    "tailwindcss": "^3.4.16",
    "ts-loader": "^9.5.1",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.17.0",
    "webpack-cli": "^5.1.4"
  },
  "description": "My webpack project"
}
